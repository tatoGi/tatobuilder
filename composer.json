{
    "name": "tatobuilder/formbuilder",
    "description": "Form builder for Laravel",
    "type": "laravel-package",
    "keywords": ["form", "build", "builder", "laravel"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tatobuilder\\FormBuilder\\": "src/"
        }
    },
    "extra": {
        "laravel": {
           "providers": [
            "Tatobuilder\\FormBuilder\\Providers\\FormBuilderServiceProvider"
        ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/tatoGi/tatobuilder"
        }
    ],
    "authors": [
        {
            "name": "Tato Laperashvili",
            "email": "tato.laperashvili95@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true, 
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0" 
    }
}
